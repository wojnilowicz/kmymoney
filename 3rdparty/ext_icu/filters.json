{
  "localeFilter": {
    "filterType": "locale",
    "includeChildren": false,
    "includeScripts": false,
    "whitelist": [
      "en"
    ]
  },
  
  "strategy": "additive",
  
  "featureFilters": {
    "cnvalias": "include",
    "misc": {
      "filterType": "regex",
      "whitelist" : [
      "timezoneTypes"
      ]
    },
    "conversion_mappings": {
      "filterType": "regex",
      "whitelist" : [
        "^ibm-1373.*", "Big5",
        "^ibm-1375.*", "Big5-HKSCS",
        "^ibm-949.*", "CP949",
        "^ibm-954.*", "EUC-JP",
        "^ibm-970.*", "EUC-KR",
        "^ibm-1051.*", "HP-ROMAN8",
        "gb18030",
        "^ibm-850.*", "IBM 850",
        "^ibm-866.*", "IBM 866",
        "^ibm-874.*", "IBM 874",
        "^ibm-91(2|3|4|5).*", "ISO 8859-(2|3|4|5)",
        "^ibm-1089.*", "ISO 8859-6",
        "^ibm-9005.*", "ISO 8859-7",
        "^ibm-5012.*", "ISO 8859-8",
        "^ibm-920.*", "ISO 8859-9",
        "^iso-8859.*", "ISO 8859-(10|11|14)",
        "^ibm-92(1|3).*", "ISO 8859-(13|15)",
        "^ibm-878.*", "KOI8-R",
        "^ibm-1168.*", "KOI8-U",
        "^macos.*", "Macintosh",
        "^ibm-943*", "Shift-JIS",
        "^windows-874-2000", "TIS-620",
        "^ibm-53(4|5).*", "windows-125(0|1|2|3|4|8)",
        "^ibm-944(7|8|9).*", "windows-125(5|6|7)"
      ]
      }
  }  
}
